Algoritmo "consumo"


Var

      distancia : inteiro
      combustivel, media : real


Inicio

         escreva ("Distancia percorrida: ")
         leia (distancia)
         escreva ("Combust√≠vel gasto: ")
         leia (combustivel)
         
         
